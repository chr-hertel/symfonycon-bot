{
    "name": "stoffel/symfonycon-bot",
    "type": "project",
    "description": "Simple Telegram bot for SymfonyCon",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4.2",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.13.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^1.19.3",
        "symfony/framework-bundle": "6.1.*",
        "symfony/messenger": "6.1.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "6.1.*",
        "symfony/property-info": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/serializer": "6.1.*",
        "symfony/telegram-notifier": "6.1.*",
        "symfony/yaml": "6.1.*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "deployer/deployer": "^7.0.2@RC",
        "friendsofphp/php-cs-fixer": "^3.4",
        "infection/infection": "^0.25.6",
        "phpstan/phpstan": "^1.8.9",
        "phpstan/phpstan-doctrine": "^1.3.16",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/debug-bundle": "6.1.*",
        "symfony/phpunit-bridge": "6.1.*",
        "symfony/stopwatch": "6.1.*",
        "symfony/twig-bundle": "6.1.*",
        "symfony/var-dumper": "6.1.*",
        "symfony/web-profiler-bundle": "6.1.*"
    },
    "config": {
        "platform": {
            "php": "8.1.6"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
