{
    "name": "stoffel/symfonycon-bot",
    "type": "project",
    "description": "Simple Telegram bot for SymfonyCon",
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2@beta",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.5.11@dev",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/property-access": "5.0.*",
        "symfony/property-info": "5.0.*",
        "symfony/serializer": "5.0.*",
        "symfony/telegram-notifier": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "easycorp/easy-log-handler": "^1.0.7",
        "phpstan/phpdoc-parser": "@dev",
        "phpstan/phpstan": "^0.12.0@dev",
        "phpstan/phpstan-phpunit": "^0.12.0@dev",
        "sensiolabs/security-checker": "^6.0",
        "symfony/browser-kit": "5.0.*",
        "symfony/css-selector": "5.0.*",
        "symfony/debug-bundle": "5.0.*",
        "symfony/phpunit-bridge": "5.0.*",
        "symfony/stopwatch": "5.0.*",
        "symfony/twig-bundle": "5.0.*",
        "symfony/var-dumper": "5.0.*",
        "symfony/web-profiler-bundle": "5.0.*"
    },
    "config": {
        "platform": {
            "php": "7.3.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
